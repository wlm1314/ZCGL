<resources>

    <style name="custom_dialog" parent="Theme.AppCompat.Dialog">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#1296db</item>
    </style>

    <style name="DialogStyleBottom" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/app_bg</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="cell">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:minHeight">@dimen/dimen_55</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/dimen_10</item>
        <item name="android:paddingTop">@dimen/dimen_6</item>
        <item name="android:paddingBottom">@dimen/dimen_6</item>
        <item name="android:paddingRight">@dimen/dimen_10</item>
    </style>

</resources>
